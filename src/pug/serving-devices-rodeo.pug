- var fork = '../img/rodeo-fork.webp'
- var knive = '../img/rodeo-knive.webp'
- var set = '../img/rodeo-set.webp'

doctype html
html(lang="ru")
    head
        meta(charset="UTF-8") 
        meta(name='viewport' content='width=device-width, initial-scale=1.0')
        include modules/favicon-manifest
        title Приборы для стейка RODEO
    body
 
        include modules/header

        main(class="main goods-content__main sd-rodeo") 
            .wr-bread-crumbs
                include modules/bread-crumbs

            .banner.sd-rodeo__banner

            
            .goods-content__wrapper-main-content

                aside.aside.goods-content__aside
                    .goods-content__wr-filter
                        .filter
                            ul.filter-list
                                li.filter__main-item-static
                                    .filter__main-item-static-title Категория
                                    ul.filter__static-sub-list
                                        li.filter__sub-item-title-box.filter__sub-item-title-box_two-rows
                                            a(href="#0" title="").filter__sub-item-title.filter__sub-item-title_two-rows
                                                span Все специальные 
                                                span сервировочные приборы
                                            .filter__sub-item-box.filter__sub-item-box_two-rows
                                        li.filter__sub-item-title-box 
                                            a(href="#0" title="").filter__sub-item-title Приборы для стейка RODEO
                                            .filter__sub-item-box
                            .filter__wr-reset-button
                                button.filter__reset-button Сбросить фильтры

                .wr-number-pages.wr-number-pages-top
                    include modules/number-pages

                article.goods-content__wr-content 
                    ul.good-card__wr-card-list  
                        li.good-card__wr-card-item 
                            a(href="./page-vase.html" title="").good-card__wrapper-link
                                .good-card__card-content 
                                    .good-card__wr-card-img-line 
                                        .good-card__wr-card-img
                                            img(src=fork alt="").good-card__card-img
                                        .good-card__wr-card-line
                                    .good-card__wr-card-title  
                                        h3.good-card__card-title Barista Cup 0.35l
                                .good-card__card-link-button

                        li.good-card__wr-card-item 
                            a(href="#0" title="").good-card__wrapper-link
                                .good-card__card-content 
                                    .good-card__wr-card-img-line 
                                        .good-card__wr-card-img
                                            img(src=knive alt="").good-card__card-img
                                        .good-card__wr-card-line
                                    .good-card__wr-card-title  
                                        h3.good-card__card-title Barista Cup 0.35l
                                .good-card__card-link-button

                        li.good-card__wr-card-item 
                            a(href="#0" title="").good-card__wrapper-link
                                .good-card__card-content 
                                    .good-card__wr-card-img-line 
                                        .good-card__wr-card-img
                                            img(src=set alt="").good-card__card-img
                                        .good-card__wr-card-line
                                    .good-card__wr-card-title  
                                        h3.good-card__card-title Barista Cup 0.35l
                                .good-card__card-link-button

                        

        include modules/footer